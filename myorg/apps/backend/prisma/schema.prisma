generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "linux-musl-arm64-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  username     String  @unique
  email        String  @unique
  password     String
  phone        String?
  profileImage String?
  role         String

  @@map("User")
}
